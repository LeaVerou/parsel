{
  "name": "parsel-js",
  "version": "1.0.3",
  "description": "A tiny, permissive CSS selector parser",
  "type": "module",
  "main": "dist/cjs/parsel.min.js",
  "module": "dist/parsel.min.js",
  "types": "./dist/parsel.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/parsel.js",
      "import": "./dist/parsel.min.js",
      "require": "./dist/cjs/parsel.min.js",
      "types": "./dist/parsel.d.ts",
      "umd": "./dist/umd/parsel.min.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "http-server -o index.html -c-1",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "build": "npx rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeaVerou/parsel.git"
  },
  "keywords": [
    "CSS",
    "selectors"
  ],
  "author": "Lea Verou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LeaVerou/parsel/issues"
  },
  "homepage": "https://projects.verou.me/parsel",
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@rollup/plugin-typescript": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "gts": "^3.1.1",
    "http-server": "^14.1.1",
    "jest": "^29.4.3",
    "rollup": "^2.49.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
